{
  "extends": "tsconfig/base.json",
  "path": {
    "@/*": ["./src/*"],
    "@@/*": ["./src/.umi/*"]
  },
  "include": [
    "mock/**/*",
    "src/**/*",
    "typings/**/*",
    "config/**/*",
    ".eslintrc.js",
    "mock/*"
  ],
  "exclude": ["src/.umi/*"],
  "compilerOptions": {
    "jsx": "react-jsx"
  }
}
